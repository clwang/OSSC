<table>
<thead>
  <tr>
    <th> Project name </th>
    <th> Title </th>
    <th> Description </th>
    <th> Point Value </th>
    <th> Create pull request </th>
  </tr> 
</thead>
<tbody>
<% @tasks.each do |task| %>
  <tr>
    <td><%= task.project.name %></td>
    <td><%= task.title %></td>
    <td><%= task.description %></td>
    <td><%= task.point_value %></td>
    <td><%= button_to 'Pull Request', { :action => 'new', :controller => 'pull_requests' }, { :method => :get } %></td>
  </tr>
<% end %>
</tbody>
</table>  